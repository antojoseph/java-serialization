        <activity
            android:name="com.example.serialization.MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
        
         <!-- Goolge API Key -->
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDqELhB2t_w-R_fifWuGc8K1BF6XD3LU9k" />
    </application>

</manifest>
